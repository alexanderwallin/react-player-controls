{
  "name": "react-player-controls-docs",
  "scripts": {
    "js": "browserify src/demos.js -t babelify -o assets/app.js",
    "js:watch": "watchify src/demos.js -t babelify -o assets/app.js",
    "serve": "live-server --port=8293",
    "start": "npm-run-all --parallel '*:watch' 'serve'",
    "build": "npm-run-all --parallel 'pug' 'js'"
  },
  "private": true,
  "dependencies": {
    "prismjs": "^1.5.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0-beta.1",
    "browserify": "^13.1.0",
    "live-server": "^1.1.0",
    "livereload": "^0.5.0",
    "npm-run-all": "^3.0.0",
    "watchify": "^3.11.0"
  }
}
