{
  "name": "react-player-controls-docs",
  "scripts": {
    "build": "browserify src/demos.js -t babelify -o assets/app.js",
    "build:watch": "watchify src/demos.js -t babelify -o assets/app.js",
    "dev": "npm-run-all --parallel '*:watch' 'start'",
    "start": "live-server --port=8293"
  },
  "private": true,
  "dependencies": {
    "prismjs": "^1.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0-beta.1",
    "browserify": "^16.5.2",
    "live-server": "^1.2.1",
    "livereload": "^0.9.1",
    "npm-run-all": "^3.0.0",
    "watchify": "^3.11.1"
  }
}
